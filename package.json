{
  "name": "level-commonform",
  "description": "store a library of Common Forms in a LevelUp-compatible datastore",
  "version": "0.0.0-prerelease",
  "author": "Kyle E. Mitchell <kyle@kemitchell.com> (http://kemitchell.com)",
  "bugs": "https://github.com/commonform/level-commonform/issues",
  "dependencies": {
    "async": "^0.9.0",
    "capitalize": "^0.5.0",
    "clone": "^1.0.2",
    "commonform-normalize": "^0.4.0",
    "commonform-predicate": "^0.4.0",
    "commonform-serialize": "^0.4.0",
    "commonform-validate": "^0.4.0",
    "through2": "^0.6.5"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "check-versions": "1.x",
    "concat-stream": "^1.4.8",
    "deep-eql": "^0.1.3",
    "istanbul": "^0.3.5",
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "levelup": "^0.19.0",
    "memdown": "^1.0.0",
    "mocha": "^2.1.0",
    "mockery": "^1.4.0",
    "tap": "^0.7.1"
  },
  "homepage": "https://commonform.github.io",
  "keywords": [
    "contracts",
    "digest",
    "document assembly",
    "knowledge management",
    "law",
    "levelup"
  ],
  "license": "Apache-2.0",
  "main": "source/index",
  "repository": "commonform/level-commonform",
  "scripts": {
    "check": "check-versions --check-module",
    "coverage": "istanbul cover _mocha && istanbul check-coverage coverage/coverage.json",
    "lint": "jshint source test && jscs source test",
    "pre-commit": "npm run lint && npm run check && npm run coverage",
    "test": "tap test/tap && mocha",
    "travis-ci": "npm run pre-commit"
  }
